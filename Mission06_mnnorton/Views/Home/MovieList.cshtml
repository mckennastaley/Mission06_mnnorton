@{ 
    ViewData["Title"] = "Movie List";
}
@model List<ApplicationResponse>

<table class="table table-bordered table-striped">
    <thead class="font-weight-bold">
        <tr>
            <td>Title</td>
            <td>Category</td>
            <td>Rating</td>
            <td>Year</td>
            <td>Director</td>
            <td>Edited</td>
            <td>Lent to</td>
            <td>Notes</td>
        </tr>
    </thead>

    <tbody>
        @foreach (var x in Model)
            {
                <tr>
                    <td>@x.MovieTitle</td>
                    <td>@x.Category.CategoryName</td>
                    <td>@x.Rating</td>
                    <td>@x.YearRelease</td>
                    <td>@x.Director</td>
                    <td>@x.Edited</td>
                    <td>@x.LentTo</td>
                    <td>@x.Notes</td>
                </tr>
            }
    </tbody>
</table>